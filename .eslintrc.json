{
    "extends": "airbnb",
    "plugins": [
//        "react",
//        "jsx-a11y",
//        "import"
    ],
    "rules": {
        "no-multi-spaces": "off",
        "indent": ["error", 4],
        "quotes": "off",
        "comma-dangle": "off",
        "no-console": "off",
        "react/jsx-indent": ["error", 4],
        "react/prefer-stateless-function": "off",
        "no-underscore-dangle": "off",
        "prefer-template": "off",
        "eqeqeq": "off",
        "react/prop-types": "off"
    },
    "env": {
        "browser": true
    }
}
